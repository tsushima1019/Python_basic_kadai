      "source": [
        "def calc_total_with_tax(price, tax_rate):\n",
        "    # price: 商品の金額 (例: 110)\n",
        "    # tax_rate: 税率 (%) (例: 10)\n",
        "    total = price + (price * tax_rate / 100)\n",
        "    return total\n",
        "\n",
        "# 例: 商品金額110, 消費税10%\n",
        "result = calc_total_with_tax(110, 10)\n",
        "print(\"税込金額:\", result)\n"
      ]
    }
